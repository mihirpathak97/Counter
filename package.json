{
  "name": "Counter",
  "version": "1.0.0",
  "description": "A cloud based counter",
  "main": "main.js",
  "scripts": {
    "dist": "build"
  },
  "build": {
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "author": "",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1"
  },
  "dependencies": {
    "firebase": "^4.12.1",
    "jquery": "^3.3.1"
  }
}
